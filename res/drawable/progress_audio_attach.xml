<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="2.0dip" android:color="#ffd2deed" android:dashWidth="2.0dip" android:dashGap="2.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="2.0dip" android:color="#ffd2deed" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="2.0dip" android:color="#ff5181b8" />
            </shape>
        </clip>
    </item>
</layer-list>